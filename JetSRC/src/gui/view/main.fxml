                              <ComboBox fx:id="stn1" layoutX="786.0" layoutY="139.0" onAction="#pylonChanged" prefWidth="100.0" />
                              <ComboBox fx:id="stn5" layoutX="400.0" layoutY="230.0" onAction="#pylonChanged" prefWidth="100.0" />
