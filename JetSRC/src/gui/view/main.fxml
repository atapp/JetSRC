                              <Label fx:id="labelStn8" alignment="CENTER" contentDisplay="CENTER" layoutX="116.0" layoutY="205.0" onMouseClicked="#removeStore" prefHeight="17.0" prefWidth="100.0" text="Empty" />
                              <Label fx:id="labelStn7" alignment="CENTER" contentDisplay="CENTER" layoutX="220.0" layoutY="205.0" onMouseClicked="#removeStore" prefHeight="17.0" prefWidth="92.0" text="Empty" />
                              <Label fx:id="labelStn6" alignment="CENTER" contentDisplay="CENTER" layoutX="311.0" layoutY="236.0" onMouseClicked="#removeStore" prefHeight="17.0" prefWidth="92.0" text="Empty" />
                              <Label fx:id="labelStn5" alignment="CENTER" contentDisplay="CENTER" layoutX="404.0" layoutY="268.0" onMouseClicked="#removeStore" prefHeight="17.0" prefWidth="93.0" text="Empty" />
                              <Label fx:id="labelStn4" alignment="CENTER" contentDisplay="CENTER" layoutX="499.0" layoutY="244.0" onMouseClicked="#removeStore" prefHeight="17.0" prefWidth="92.0" text="Empty" />
